<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.oneback">


    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/firebase_lockup_400"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".commons.BaseActivity" />
        <activity android:name=".commons.EmailPasswordActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".CadastroActivity" />
        <activity android:name=".MainActivityTo" />
        <activity android:name=".NewPostActivity" />
        <activity android:name=".PostDetailActivity" />

        <activity
            android:name=".commons.GoogleSignInActivity"
            android:label="@string/label_google_sign_in" />

    </application>

</manifest>